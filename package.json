{
  "name": "billing-widget",
  "version": "1.0.0",
  "private": true,
  "description": "billing widget powered by effector",
  "main": "src/index.ts",
  "author": {
    "name": "Ilia Klimov",
    "email": "klimov@ttfb.dev",
    "url": "https://github.com/klimov-code/billing-widget"
  },
  "scripts": {
    "start": "vite",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "vite build",
    "build:no-cache": "pnpm clean:cache && pnpm build",
    "preview": "pnpm build && vite preview",
    "preview:dist": "vite preview",
    "type:check": "tsc --noEmit --skipLibCheck",
    "clean:cache": "rm -rf node_modules/.cache/ && rm -rf node_modules/.vite",
    "clean:lib": "rm -rf node_modules",
    "lint": "run-s lint:all:*",
    "lint:js": "eslint --fix",
    "lint:css": "stylelint --fix",
    "lint:all:js": "eslint --fix --cache \"**/*.{js,jsx,ts,tsx}\"",
    "lint:all:css": "stylelint --fix --cache \"**/*.{css}\" --cache-location node_modules/.cache/stylelint/",
    "format": "prettier --write --loglevel warn",
    "format:all": "pnpm format \"src/**/*.+(js|jsx|ts|tsx|json|css|html|md)\"",
    "update:deps": "pnpm update -i -L",
    "update:cssdb": "pnpm -s dlx browserslist@latest --update-db",
    "reinstall": "rm -rf node_modules && pnpm i"
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "5.0.0-alpha.105",
    "@mui/material": "^5.10.11",
    "effector": "^22.3.0",
    "effector-react": "^22.3.4",
    "nanoid": "^4.0.0",
    "patronum": "^1.12.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
    "@rollup/plugin-babel": "^6.0.2",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.24.3",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "effector-logger": "^0.13.4",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-effector": "^0.10.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "happy-dom": "^7.6.6",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "rollup": "^3.2.3",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^14.14.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.1",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "^0.24.3"
  }
}
