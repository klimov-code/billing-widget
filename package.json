{
  "name": "billing-widget",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "billing widget powered by effector",
  "main": "src/index.ts",
  "author": {
    "name": "Ilia Klimov",
    "email": "klimov@ttfb.dev",
    "url": "https://github.com/klimov-code/billing-widget"
  },
  "scripts": {
    "start": "vite",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "vite build",
    "build:no-cache": "pnpm clean:cache && pnpm build",
    "preview": "pnpm build && vite preview",
    "preview:dist": "vite preview",
    "type:check": "tsc --noEmit --skipLibCheck",
    "clean:cache": "rm -rf node_modules/.cache/ && rm -rf node_modules/.vite",
    "clean:lib": "rm -rf node_modules",
    "lint": "run-s lint:all:*",
    "lint:js": "eslint --fix",
    "lint:css": "stylelint --fix",
    "lint:all:js": "eslint --fix --cache \"**/*.{js,jsx,ts,tsx}\" --cache-location node_modules/.cache/eslint/",
    "lint:all:css": "stylelint --fix --cache \"**/*.{css}\" --cache-location node_modules/.cache/stylelint/",
    "format": "prettier --write --loglevel warn",
    "format:all": "pnpm format \"src/**/*.+(js|jsx|ts|tsx|json|css|html|md)\"",
    "update:deps": "pnpm update -i -L",
    "update:cssdb": "pnpm -s dlx browserslist@latest --update-db",
    "reinstall": "rm -rf node_modules && pnpm i"
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/lab": "5.0.0-alpha.135",
    "@mui/material": "^5.13.7",
    "effector": "^22.8.6",
    "effector-react": "^22.5.3",
    "nanoid": "^4.0.2",
    "patronum": "^1.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@effector/swc-plugin": "^0.0.10",
    "@swc/core": "^1.3.68",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-c8": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "effector-logger": "^0.14.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-effector": "^0.10.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-perfectionist": "^1.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "happy-dom": "^10.1.1",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.25",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.0",
    "rollup": "^3.26.2",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^15.10.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.1.6",
    "unplugin-swc": "^1.3.2",
    "vite": "^4.4.2",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.33.0"
  }
}
