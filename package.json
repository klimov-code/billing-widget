{
  "name": "billing-widget",
  "version": "1.0.0",
  "description": "billing widget powered by effector",
  "main": "src/index.ts",
  "author": {
    "name": "Ilia Klimov",
    "email": "klimov@ttfb.dev",
    "url": "https://github.com/klimov-code"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:no-cache": "pnpm clean:cache && pnpm build",
    "preview": "pnpm build && vite preview",
    "preview:dist": "vite preview",
    "type:check": "tsc --noEmit --skipLibCheck",
    "clean:cache": "rm -rf node_modules/.cache/ && rm -rf node_modules/.vite",
    "clean:lib": "rm -rf node_modules",
    "lint": "run-s lint:all:*",
    "lint:js": "eslint --fix",
    "lint:css": "stylelint --fix",
    "lint:all:js": "eslint --fix --cache \"**/*.{js,jsx,ts,tsx}\"",
    "lint:all:css": "stylelint --fix --cache \"**/*.{css,scss}\" --cache-location node_modules/.cache/stylelint/",
    "format": "prettier --write --loglevel warn",
    "format:all": "pnpm format \"src/**/*.+(js|jsx|ts|tsx|json|css|scss|html|md)\"",
    "update:deps": "pnpm update -i -L && pnpm update",
    "update:cssdb": "pnpm -s dlx browserslist@latest --update-db",
    "reinstall": "rm -rf node_modules && pnpm i"
  },
  "license": "MIT",
  "dependencies": {
    "are-passive-events-supported": "^1.1.1",
    "clsx": "^1.1.1",
    "effector": "^22.2.0",
    "effector-react": "^22.0.6",
    "nanoid": "^3.3.1",
    "patronum": "^1.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@rollup/plugin-babel": "^5.3.1",
    "@types/react": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@unocss/preset-typography": "^0.27.5",
    "@unocss/preset-uno": "^0.27.5",
    "@unocss/transformer-directives": "^0.27.5",
    "@vitejs/plugin-react": "^1.2.0",
    "cross-env": "^7.0.3",
    "effector-logger": "^0.13.4",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-effector": "^0.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nano-staged": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.8",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "rollup": "^2.70.0",
    "sass": "^1.49.9",
    "simple-git-hooks": "^2.7.0",
    "stylelint": "^14.5.3",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.6.2",
    "unocss": "^0.27.5",
    "vite": "^2.8.6",
    "vite-plugin-eslint": "^1.3.0",
    "vite-svg-loader": "^3.1.2"
  }
}
